<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:id="@+id/linear_layout"
    android:layout_height="match_parent"
    android:descendantFocusability="beforeDescendants"
    android:focusableInTouchMode="true"
    android:background="#FFFFFF">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/background_one"
                android:layout_width="match_parent"
                android:scaleType="fitStart"
                android:adjustViewBounds="true"
                android:layout_height="match_parent"
                android:src="@mipmap/back_check1"/>
            <TextView
                android:text="@string/details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewTextText"
                android:textAlignment="center"
                android:layout_marginTop="210dp"
                android:textSize="60sp"
                android:textStyle="bold"
                android:textColor="@android:color/white"
                android:layout_centerHorizontal="true"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewHealthFacility"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:layout_below="@+id/textViewTextText"
                android:text="@string/health_facility"
                android:textSize="42sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="600dp"
                android:id="@+id/textView" />

            <TextView
                android:text="TextView"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewHealthFacility"
                android:layout_below="@+id/textView"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewMEN"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:layout_below="@+id/textViewHealthFacility"
                android:text="@string/address"
                android:textSize="42sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textViewNew" />

            <TextView
                android:text="TextView"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewAddress"
                android:layout_below="@+id/textViewNew"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewAddress"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:id="@+id/textViewMENStatic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/MEN"
                android:textSize="42sp"
                android:layout_below="@id/textViewAddress"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:gravity="center_horizontal"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:id="@+id/textViewMEN"
                android:layout_below="@+id/textViewMENStatic"
                android:textColor="@color/colorPrimary"
                android:textSize="40sp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="18dp"
                android:textStyle="bold"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewName"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:text="@string/name"
                android:textSize="42sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewMEN"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView10" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewName"
                android:layout_below="@+id/textView10"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewDiagnose"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>

            <TextView
                android:text="@string/diagnose"
                android:textSize="42sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewAge"
                android:textColor="@color/colorPrimaryDark"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView3" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDiagnose"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_below="@+id/textView3"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewAge"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>

            <TextView
                android:text="@string/age"
                android:textSize="42sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:layout_below="@+id/textViewName"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView4" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewAge"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_below="@+id/textView4"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewNeeds"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:textSize="42sp"
                android:text="@string/needs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:layout_below="@+id/textViewDiagnose"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView5" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewNeeds"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_below="@+id/textView5"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewServe"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:textSize="42sp"
                android:text="@string/serve_to"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimaryDark"
                android:layout_below="@+id/textViewNeeds"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView6" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewServe"
                android:layout_below="@+id/textView6"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewDoctor"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:text="@string/doctor"
                android:textSize="42sp"
                android:textColor="@color/colorPrimaryDark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewServe"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView7" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDoctor"
                android:layout_below="@+id/textView7"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:textSize="40sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#d3d3d3"
                android:layout_below="@+id/textViewDate"
                android:layout_marginTop="5dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"/>
            <TextView
                android:text="@string/date"
                android:textSize="42sp"
                android:textColor="@color/colorPrimaryDark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewDoctor"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="30dp"
                android:id="@+id/textView169" />
            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewDate"
                android:layout_below="@+id/textView169"
                android:textColor="@color/colorPrimary"
                android:textStyle="bold"
                android:layout_marginTop="18dp"
                android:layout_marginStart="50dp"
                android:layout_marginBottom="30dp"
                android:textSize="40sp"/>
        </RelativeLayout>





    </ScrollView>
</LinearLayout>