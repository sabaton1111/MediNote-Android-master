<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/check_back">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@android:color/transparent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:background="@android:color/white"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:contentDescription="@string/logo"
                        android:layout_width="220dp"
                        android:layout_height="100dp"
                        android:background="@mipmap/medinote_logo_new"
                        android:layout_marginTop="65dp"
                        android:id="@+id/imageViewLogo"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" />


                    <TextView
                        android:layout_below="@+id/imageViewLogo"
                        android:text="Date"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textViewTextText" />

                    <TextView
                        android:layout_below="@+id/textViewTextText"
                        android:text="@string/health_facility"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="45dp"
                        android:id="@+id/textView" />

                    <TextView
                        android:text="TextView"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewHealthFacility"
                        android:layout_below="@+id/textView"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:text="@string/name"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textViewHealthFacility"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView10" />
                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewName"
                        android:layout_below="@+id/textView10"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:text="@string/address"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textViewName"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView3" />
                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewAddress"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_below="@+id/textView3"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:text="@string/age"
                        android:textSize="20sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_below="@+id/textViewAddress"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView4" />
                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewAge"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_below="@+id/textView4"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:textSize="20sp"
                        android:text="@string/needs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_below="@+id/textViewAge"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView5" />
                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewNeeds"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_below="@+id/textView5"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:textSize="20sp"
                        android:text="@string/serve_to"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_below="@+id/textViewNeeds"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView6" />

                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewServe"
                        android:layout_below="@+id/textView6"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:textSize="18sp"/>

                    <TextView
                        android:text="@string/doctor"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textViewServe"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="30dp"
                        android:id="@+id/textView7" />
                    <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textViewDoctor"
                        android:layout_below="@+id/textView7"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold"
                        android:layout_marginTop="18dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginBottom="30dp"
                        android:textSize="18sp"/>

                </RelativeLayout>





        </ScrollView>
    </RelativeLayout>
</LinearLayout>